{
  "name": "stillbit-dashboard",
  "version": "1.1.0",
  "description": "Stillbit",
  "private": true,

  "scripts": {
    "dev":   "parcel src/index.html --open --port 3000",
    "build": "tsc && parcel build src/index.html --dist-dir dist",
    "lint":  "eslint \"src/**/*.{ts,tsx}\"",
    "fmt":   "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "serve": "http-server dist -p 8080"
  },

  "dependencies": {
    "parcel": "^2.14.2",
    "http-server": "^14.1.1"
  },

  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.4"
  }
}
